{
  "name": "smartaccess_v2",
  "version": "2.0.0",
  "description": "Smart Access Version 2",
  "main": "app.js",
  "scripts": {
    "build_environment_babel": "babel ./tools/setEnvironment.es --out-file ./tools/dist/setEnvironment.js && babel ./tools/app_config.es --out-file ./tools/dist/app_config.js",
    "build_environment": "node ./tools/dist/setEnvironment default",
    "build-dev": "npm-run-all build_environment_babel \"build_environment -- dev\"",
    "build-test": "npm-run-all build_environment_babel \"build_environment -- test\"",
    "build-prod": "npm-run-all build_environment_babel \"build_environment -- prod\"",
    "check-node-modules": "nsp check",
    "webpack": "webpack --config webpack.config.js --progress --watch",
    "webpack-dev-server": "webpack-dev-server --open",
    "eslint": "eslint --ext .js --ext .es --ext .jsx --fix app",
    "start": "npm-run-all check-node-modules webpack-dev-server"
  },
  "author": "Wesley Duff",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "2.22.0",
    "jest": "^20.0.4",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "make-runnable": "^1.3.6",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "postcss": "^6.0.6",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5",
    "whatwg-fetch": "1.0.0"
  },
  "eslintIgnore": [],
  "eslintConfig": {
    "extends": "node_modules/react-scripts/config/eslint.js"
  }
}
