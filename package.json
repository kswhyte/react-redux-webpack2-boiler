{
  "name": "smartaccess_v2",
  "version": "2.0.0",
  "description": "Smart Access Version 2",
  "main": "app.js",
  "scripts": {
    "build_environment_babel": "babel ./tools/setEnvironment.es --out-file ./tools/dist/setEnvironment.js && babel ./tools/app_config.es --out-file ./tools/dist/app_config.js",
    "build_environment": "node ./tools/dist/setEnvironment default",
    "build-dev": "npm-run-all build_environment_babel \"build_environment -- dev\"",
    "build-test": "npm-run-all build_environment_babel \"build_environment -- test\"",
    "build-prod": "npm-run-all build_environment_babel \"build_environment -- prod\"",
    "check-node-modules": "nsp check",
    "webpack": "webpack --config webpack.config.js --progress --watch",
    "webpack-dev-server": "webpack-dev-server --open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint": "eslint --ext .es,.jsx app",
    "test": "jest --no-cache",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open ./coverage/lcov-report/index.html",
    "eslint": "eslint --ext .js --ext .es --ext .jsx --fix app",
    "start": "npm-run-all check-node-modules webpack-dev-server"
  },
  "author": "Wesley Duff",
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "2.1.14",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "2.22.0",
    "jasmine-enzyme": "^3.1.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.5.1",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "make-runnable": "^1.3.6",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "postcss": "^6.0.6",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.5.1",
    "react-addons-update": "^15.5.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "2.5.4",
    "sinon": "^2.2.0",
    "storybook-addon-specifications": "^1.0.16",
    "style-loader": "0.13.1",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.1.0"
  },
  "jest": {
    "rootDir": "src/components",
    "setupFiles": [
      "../../.storybook/__conf__/config.js"
    ],
    "automock": false,
    "notify": true,
    "setupTestFrameworkScriptFile": "../../node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/components/*.{es, js,jsx}",
      "src/components/**/*.{es, js,jsx}",
      "!**/helpers/**"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "es"
    ],
    "moduleNameMapper": {
      "^@kadira/storybook$": "<rootDir>/__mocks__/@kadira/storybook"
    },
    "unmockedModulePathPatterns": [
      "react",
      "redux",
      "enzyme",
      "react-addons-test-utils"
    ],
    "testPathIgnorePatterns": [
      "spec.jsx"
    ],
    "transform": {
      ".es$": "../../node_modules/babel-jest",
      ".jsx$": "../../node_modules/babel-jest",
      ".js$": "../../node_modules/babel-jest"
    }
  },
  "eslintIgnore": [],
  "eslintConfig": {
    "extends": "node_modules/react-scripts/config/eslint.js"
  }
}
