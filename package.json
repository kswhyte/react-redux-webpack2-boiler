{
  "name": "smartaccess_v2",
  "version": "2.0.0",
  "description": "Smart Access Version 2",
  "main": "app.js",
  "scripts": {
    "build_environment_babel": "babel  ./tools/buildScripts --out-dir  ./tools/dist",
    "build_environment": "node ./tools/dist/setEnvironment default",
    "add-mocks": "npm-run-all build_environment_babel \"build_environment -- add-mocks\"",
    "remove-mocks": "npm-run-all \"build_environment -- remove-mocks\"",
    "build-dev": "npm-run-all build_environment_babel \"build_environment -- dev\"",
    "build-test": "npm-run-all build_environment_babel \"build_environment -- test\"",
    "build-prod": "npm-run-all build_environment_babel \"build_environment -- prod\"",
    "format": "prettier --write --single-quote --print-width=120 --tab-width=2 \"js/**/*.{js,jsx}\"",
    "flow": "flow",
    "check-node-modules": "nsp check",
    "build-core-css": "postcss -u postcss-import -u postcss-cssnext -o src/public/css/dist/core_build.css src/public/css/core.css",
    "build-core-css-prod": "postcss -u postcss-import -u postcss-cssnext -o dist/core_build.css src/public/css/core.css",
    "webpack": "webpack -d --config webpack.config.js --progress",
    "webpack-Watch": "webpack --config webpack.config.js --progress --watch",
    "webpack-dev-server": "webpack-dev-server",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook",
    "test": "jest --no-cache",
    "test-jenkins": "jest --updateSnapshot",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open ./coverage/lcov-report/index.html",
    "lintWatch": "esw webpack.config.* src tools/buildScripts --ext .es6,.js,.jsx --color -w",
    "lint": "esw webpack.config.* src tools/buildScripts --ext .es6,.js,.jsx --color",
    "prestart": "babel-node tools/buildScripts/startMessage",
    "start": "npm-run-all check-node-modules lint build-core-css webpack webpack-dev-server",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist build-core-css-prod test-jenkins lint",
    "build": "babel-node tools/buildScripts/build_prod.es6"
  },
  "author": "Wesley Duff",
  "license": "ISC",
  "devDependencies": {
    "@storybook/react": "3.1.9",
    "babel-core": "6.17.0",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.5",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-register": "6.16.3",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "deep-equal-ident": "^1.1.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.50.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^20.0.4",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "3.9.3",
    "lodash-contrib": "393.0.1",
    "make-runnable": "^1.3.6",
    "nock": "8.1.0",
    "npm-run-all": "^3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "prettier": "^1.5.3",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.5.1",
    "react-addons-update": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-prop-types": "^0.4.0",
    "sinon": "^2.4.1",
    "storybook-addon-specifications": "^2.1.0",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-sources": "^1.0.1",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "@hg/hg-common-toolz": "git+https://git.healthgrades.com/blue-falcon/hg-common-toolz.git#release",
    "@hg/search-ui": "git+https://git.healthgrades.com/blue-falcon/search-ui.git#release-2.5.0",
    "@hg/styleguide-kit": "git+https://git.healthgrades.com/blue-falcon/styleguide-kit.git#dev-release",
    "@hg/three-ui": "git+https://git.healthgrades.com/blue-falcon/three-ui.git#v1.8.10",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "2.1.14",
    "expect": "^1.20.2",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^3.5.1",
    "mock-local-storage": "^1.0.4",
    "moment": "^2.18.1",
    "npm-run-all": "^3.1.1",
    "postcss": "^6.0.7",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-url": "^7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-thunk": "^2.2.0",
    "rimraf": "2.5.4",
    "superagent": "^3.5.2"
  },
  "jest": {
    "rootDir": "src/",
    "setupFiles": [
      "../.storybook/__conf__/config.js"
    ],
    "automock": false,
    "notify": true,
    "setupTestFrameworkScriptFile": "../node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/components/*.{es6, js,jsx}",
      "src/components/**/*.{es6, js,jsx}",
      "!**/helpers/**",
      "src/_reducers/*.{es6, js,jsx}",
      "src/actions/*.{es6, js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "es",
      "es6",
      "less",
      "css"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "redux",
      "enzyme",
      "react-addons-test-utils",
      "react-router",
      "react-router-dom"
    ],
    "moduleNameMapper": {
      "^@hg/three-ui": "<rootDir>/components/HgStub/HgStub.jsx",
      "\\.(css|less|png|jpg)$": "<rootDir>/components/HgStub/HgStub.jsx"
    },
    "testPathIgnorePatterns": [
      "spec.jsx"
    ],
    "transform": {
      ".es6$": "../node_modules/babel-jest",
      ".es$": "../node_modules/babel-jest",
      ".jsx$": "../node_modules/babel-jest",
      ".js$": "../node_modules/babel-jest"
    }
  }
}
